<div class="collapsible-header active"><i class="material-icons">schedule</i>
    <span class="badge"><span data-bind="time">--:--</span> / <span data-bind="height">--:--</span></span>
</div>
<div class="collapsible-body container">
    <form data-subview="form"></form>
    <div class="row">
        <div class="col s4 offset-s2">
            <div data-action="save" class="waves-effect waves-light btn indigo">
                <i class="material-icons left">save</i>Save
            </div>
        </div>
        <div class="col s4">
            <div data-action="remove" class="waves-effect waves-light btn indigo">
                <i class="material-icons right">delete</i>Delete
            </div>
        </div>
    </div>
</div>
